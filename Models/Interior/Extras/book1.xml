<?xml version="1.0"?>

<PropertyList>
 <path>book1.ac</path>

<!-- -->
 <animation>
  <type>pick</type>
   <object-name>cover1</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
   <binding>
<condition>
  <equals>
    <property>an24/Extras/book1_cover</property>
    <value>0.0</value>
  </equals>
</condition>
    <command>property-interpolate</command>
    <property>an24/Extras/book1_cover</property>
    <value>1.0</value>
    <time>0.4</time>
   </binding>

   <binding>
<condition>
  <equals>
    <property>an24/Extras/book1_cover</property>
    <value>1.0</value>
  </equals>
</condition>
    <command>property-interpolate</command>
    <property>an24/Extras/book1_cover</property>
    <value>0.0</value>
    <time>0.4</time><!-- 2 seconds to 0 from 1, 0.5, -1 etc. -->
   </binding>
   </action>
 </animation>

 <animation>
  <object-name>cover2</object-name>
  <type>rotate</type>
  <property>an24/Extras/book1_cover</property>
  <factor>-80</factor>
  <center>
   <x-m>-0.0627</x-m>
   <y-m>0</y-m>
   <z-m>-0.0069</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <object-name>cover1</object-name>
