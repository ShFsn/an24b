   <z-m>-0.0069</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <object-name>cover1</object-name>
  <type>rotate</type>
  <property>an24/Extras/book1_cover</property>
  <factor>-80</factor>
