<!-- Panel lighting -->

<model>
 <path>knob_panel-lighting.xml</path>
  <offsets>
    <x-m>0.004</x-m>
    <y-m>-0.11</y-m>
    <z-m>0.21</z-m>
  </offsets>
</model>

 <animation>
   <type>material</type>
   <object-name>electrical_panel</object-name>
   <object-name>screws_etc</object-name>
   <emission>
     <red>0.8</red>
     <green>0.7</green>
     <blue>0.6</blue>
     <factor-prop>controls/lighting/panel-elec_red</factor-prop>
   </emission>
 </animation>

 <animation>
   <type>material</type>
   <object-name>lightplate</object-name>
