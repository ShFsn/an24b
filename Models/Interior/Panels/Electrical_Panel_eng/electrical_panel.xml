<!-- STG-18TMO Voltage Regulators VS-25B -->

 <animation>
   <type>knob</type>
   <object-name>stg-18l_voltreg</object-name>
   <property>an24/Electrical_Panel/stg-18l_voltreg</property>
   <factor>-2000</factor>
  <axis>
    <x>1.0</x>
    <y>0.0</y>
    <z>0.0</z>
  </axis>
  <center>
    <x-m>0.0</x-m>
    <y-m>-0.0781</y-m>
    <z-m>-0.1256</z-m>
  </center>
    <action>
     <binding>
      <command>property-adjust</command>
      <property>an24/Electrical_Panel/stg-18l_voltreg</property>
      <factor>-0.01</factor>
      <min>0.83</min>
      <max>1.23</max>
      <wrap>false</wrap>
     </binding>
    </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>stg18regl</tooltip-id>
      <label>Regulate STG-18
    Voltage</label>
    </binding>
   </hovered>
 </animation>

 <animation>
   <type>knob</type>
   <object-name>stg-18r_voltreg</object-name>
   <property>an24/Electrical_Panel/stg-18r_voltreg</property>
